{
  "name": "sqltyper",
  "version": "1.0.1",
  "description": "Typed SQL queries in PostgreSQL",
  "main": "dist/src/index.js",
  "repository": "https://github.com/akheron/sqltyper",
  "author": "Petri Lehtinen <petri@digip.org>",
  "license": "MIT",
  "keywords": [
    "database",
    "postgres",
    "postgresql",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "bin": {
    "sqltyper": "dist/src/cli.js"
  },
  "scripts": {
    "sql": "ts-node src/cli.ts --target postgres --module '../postgres' --prettify src/sql/",
    "sql:check": "yarn run sql --check",
    "sql:watch": "yarn run sql --watch",
    "lint": "eslint '**/*.ts' && prettier --check \"**/*.{json,md}\"",
    "lint:fix": "eslint --fix '**/*.ts' && prettier --write '**/*.{json,md}'",
    "build": "tsc -p tsconfig.dist.json",
    "clean": "rm -rf dist/",
    "prepublishOnly": "yarn clean && yarn build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/ramda": "^0.27.3",
    "@types/wrap-ansi": "^3.0.0",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "prettier": "^2.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.8.1"
  },
  "dependencies": {
    "buffer-writer": "^2.0.0",
    "camelcase": "^6.0.0",
    "fp-ts": "^2.5.3",
    "node-watch": "^0.6.3",
    "packet-reader": "^1.0.0",
    "ramda": "^0.27.0",
    "typescript": ">=3.8",
    "wrap-ansi": "^7.0.0",
    "yargs": "^15.3.1"
  }
}
