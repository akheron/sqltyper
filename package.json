{
  "name": "sqltyper",
  "version": "0.2.1",
  "description": "Typed SQL queries in PostgreSQL",
  "main": "dist/index.js",
  "repository": "https://github.com/akheron/sqltyper",
  "author": "Petri Lehtinen <petri@digip.org>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "sqltyper": "dist/src/cli.js"
  },
  "scripts": {
    "sql": "ts-node src/cli.ts --prettify --pg-module ../pg src/sql/",
    "sql:check": "yarn run sql --check",
    "sql:watch": "yarn run sql --watch",
    "build": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@types/camelcase": "^5.2.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "@types/ramda": "^0.26.21",
    "@types/wrap-ansi": "^3.0.0",
    "@types/yargs": "^13.0.2",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "dependencies": {
    "buffer-writer": "^2.0.0",
    "camelcase": "^5.3.1",
    "fp-ts": "^2.0.5",
    "node-watch": "^0.6.3",
    "packet-reader": "^1.0.0",
    "pg-connection-string": "^2.1.0",
    "pg-pool": "^2.0.7",
    "pg-types": "^2.2.0",
    "pgpass": "^1.0.2",
    "ramda": "^0.26.1",
    "typed-parser": "^0.1.4",
    "typescript": "^3.6.2",
    "wrap-ansi": "^6.0.0",
    "yargs": "^14.0.0"
  }
